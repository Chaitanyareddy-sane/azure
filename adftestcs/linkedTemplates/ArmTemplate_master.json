{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adftestcs"
		},
		"AzureDataLake_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLake_ls'"
		},
		"AzureSql_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSql_ls'"
		},
		"LS_adls_gen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_adls_gen2'"
		},
		"sql_newserver_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sql_newserver'"
		},
		"AzureDataLake_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagetestcsr.dfs.core.windows.net/"
		},
		"LS_adls_gen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagetestcsr.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adftestcs_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLake_ls_accountKey": {
						"value": "[parameters('AzureDataLake_ls_accountKey')]"
					},
					"AzureSql_ls_connectionString": {
						"value": "[parameters('AzureSql_ls_connectionString')]"
					},
					"LS_adls_gen2_accountKey": {
						"value": "[parameters('LS_adls_gen2_accountKey')]"
					},
					"sql_newserver_connectionString": {
						"value": "[parameters('sql_newserver_connectionString')]"
					},
					"AzureDataLake_ls_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLake_ls_properties_typeProperties_url')]"
					},
					"LS_adls_gen2_properties_typeProperties_url": {
						"value": "[parameters('LS_adls_gen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}